# NPM ignore file - what NOT to publish to npm registry

# Source files
**/*.spec.ts
**/*.test.ts
**/test/
apps/*/test/

# Development files
.env
.env.example
.vscode/
.idea/

# Build and dependency directories
node_modules/
coverage/
.nyc_output/

# Configuration files
tsconfig.*.json
jest.config.js
.eslintrc.js
.prettierrc

# Logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# OS files
.DS_Store
Thumbs.db

# Documentation (if you want to exclude)
docs/
SECURITY_AUDIT_SUMMARY.md

# TypeScript build info
*.tsbuildinfo

# Temporary files
*.tmp
*.temp
*.swp

# Git files
.git/
.gitignore
.gitattributes
